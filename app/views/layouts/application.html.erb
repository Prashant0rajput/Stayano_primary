   <%= link_to  ('<i class="material-icons">group</i>').html_safe, users_path ,:class =>'side_buttons'%> 
    <%= link_to ('<i class="material-icons">settings</i>').html_safe, user_settings_path, method: :get , :class =>'side_buttons'%>
    <%= link_to ('<i class="material-icons">power_settings_new</i>').html_safe, destroy_user_session_path, method: :get , :class =>'side_buttons'%>
  


</div>

 




