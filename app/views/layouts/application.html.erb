  <div class="dropmenu" , id="dropmenu">
      <span style="font-size:50px;cursor:pointer" onclick="openNav()">&#9776;</span>

  </div>

   

   

    <% end -%>
 










<div id="mySidenav" class="sidenav">

  <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
