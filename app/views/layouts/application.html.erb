    </div>

    <div class="brand">
      <p> <%= link_to "stayano" , new_user_session_url , id: "brand"   %> </p>
    </div>
</div>


    <% if user_signed_in? %>
 



<div class="fixed-action-btn horizontal">
    <a class="btn-floating btn-large red">
